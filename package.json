{
  "name": "graflint",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 5556",
    "build": "vite build",
    "preview": "vite preview",
    "ts": "tsc --version && tsc --project tsconfig.app.json",
    "fmt": "prettier --write . --ignore-path .gitignore --experimental-fast-cli",
    "fmt:check": "prettier --check . --ignore-path .gitignore --experimental-fast-cli",
    "lint": "eslint --version && eslint . --quiet",
    "lint:fix": "eslint --version && eslint . --quiet --fix"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwindcss": "^4.1.18",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/bun": "^1.3.5",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^16.5.0",
    "prettier": "^3.7.4",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.52.0",
    "vite": "^7.3.0"
  },
  "packageManager": "bun@1.3.5"
}
